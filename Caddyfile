gouv.fa {
	encode zstd gzip
	file_server {
		precompressed br
	}
	root * /srv/sites/gouv.fa/public
	tls /srv/certs/gouv.fa/public.pem /srv/certs/gouv.fa/private.pem
}
http://gouv.fa {
	encode zstd gzip
	file_server {
		precompressed br
	}
	root * /srv/sites/gouv.fa/public
}
www.gouv.fa {
	encode zstd gzip
	redir https://gouv.fa{uri}
	tls /srv/certs/gouv.fa/public.pem /srv/certs/gouv.fa/private.pem
}
http://farer.,
http://fa.,
http://example.fa,
http://www.gouv.fa {
	encode zstd gzip
	redir http://gouv.fa{uri}
}

farer.group {
	encode zstd gzip
	file_server {
		precompressed br
	}
	root * /srv/sites/farer.group/public
}
www.farer.group {
	encode zstd gzip
	redir https://farer.group{uri}
}
